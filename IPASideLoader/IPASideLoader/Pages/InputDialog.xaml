<Window x:Class="IPASideLoader.Pages.InputDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IPASideLoader.Pages"
        mc:Ignorable="d"
        Title="InputDialog" Height="115.219" Width="428.75">
    <StackPanel>
        <TextBlock TextWrapping="WrapWithOverflow"  x:Name="DialogTextBox" Text="Enter Password" />
        <TextBox x:Name="ResponseTextBox" />
        <Button Content="OK" Click="Button_Click"  />
    </StackPanel>
</Window>
